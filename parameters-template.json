{
  "cluster": {
    "api_server": "<cluster_api_url>",
    "username": "kubeadmin",
    "password": "<kubeadmin_password>",
    "token": "",
    "storageclass": {
      "block": "ocs-storagecluster-ceph-rbd",
      "file": "ocs-storagecluster-cephfs"
    },
    "version": "4.16"
  },
  "cpd": {
    "version": "5.1.3",
    "edition": "EE",
    "cpdcli_version": "14.1.3",
    "namespace": {
      "cpd_operator": "cpd-operator",
      "cpd_operands": "cpd-instance",
      "cert_manager": "ibm-cert-manager",
      "scheduling_service": "ibm-cpd-scheduler",
      "license_service": "ibm-licensing"
    },
    "ibm_entitlement_key": "<ibm_entitlement_key>"
  },
  "rhoai": {
    "version": "2.16",
    "namespace": "redhat-ods-operator"
  },
  "rhnfdt": {
    "namespace": "openshift-nfd"
  },
  "watsonxai": {
    "install": "yes",
    "tuning_disabled": "false",
    "lite_version": "false"
  },
  "watsonxgov": {
    "install": "yes",
    "install_type": "all",
    "factsheet": "true",
    "openscale": "true",
    "openpages": {
      "enable": "true",
      "global_search": "false",
      "existing": {
        "existing_instance": "false",
        "namespace": "openpagesinstance-cr",
        "db_type": "internal",
        "db": "Db2",
        "db_secret": "db2_secret"
      }
    }
  },
  "watsonxdata": {
    "install": "yes"
  },
  "directories": {
    "bin_dir": "/usr/local/bin",
    "cpd_dir": "$HOME/cpd/cpd-cli-workspace",
    "tmp_dir": "/tmp"
  }
}
